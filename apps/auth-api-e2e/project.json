{
  "name": "@challah-social/auth-api-e2e",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "implicitDependencies": ["@challah-social/auth-api"],
  "targets": {
    "e2e": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{e2eProjectRoot}"],
      "options": {
        "jestConfig": "apps/auth-api-e2e/jest.config.cts",
        "passWithNoTests": true
      },
      "dependsOn": [
        "@challah-social/auth-api:build",
        "@challah-social/auth-api:serve"
      ]
    }
  }
}
